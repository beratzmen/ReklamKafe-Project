<!doctype html>
<html lang="en">
<head>

    @*<script async src="//pagead2.googlesyndication.com/     bu remarklı kalsın
        pagead/js/adsbygoogle.js"></script>
            <script>
                (adsbygoogle = window.adsbygoogle || []).push({
                    google_ad_client: "pub-2717829644533925",
                    enable_page_level_ads: true
                });
            </script>*@
    <script data-ad-client="ca-pub-2717829644533925" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js">
        <ins class="adsbygoogle"
            style="display:block"
            data-ad-format="fluid"
            data-ad-layout-key="+2g+q3+r+21+4v"
            data-ad-client="ca-pub-2717829644533925"
            data-ad-slot="1586169361"></ins>
    </script>

    <script async custom-element="amp-ad" src="https://cdn.ampproject.org/v0/amp-ad-0.1.js"></script>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="keywords" content="ücretsiz, ürün, bedava, araç gereç" />
    <meta name="description" content="Reklam Kafe, size sunulan reklamları izlediğinizde puan kazandırıp ve bu puanlar ile bedava ürün kazanmanızı sağlar." />
    <meta name="robots" content="all" />
    <meta name="revisit-after" content="6 days">

    <link rel="icon" href="~/Content/img/favicon.png" type="~/Content/image/png">
    <title>Reklam Kafe | Ücretsiz Ürün Bedava Araç Gereç | @ViewBag.Title</title>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.css">

    <link rel="stylesheet" href="~/Content/vendors/linericon/style.css">
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/vendors/owl-carousel/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/vendors/lightbox/simpleLightbox.css">
    <link rel="stylesheet" href="~/Content/vendors/nice-select/css/nice-select.css">
    <link rel="stylesheet" href="~/Content/vendors/animate-css/animate.css">
    <link rel="stylesheet" href="~/Content/vendors/jquery-ui/jquery-ui.css">
    <!-- main css -->
    <link rel="stylesheet" href="~/Content/css/style.css">
    <link rel="stylesheet" href="~/Content/css/responsive.css">
    <link href="~/Content/css/Main/global.css" rel="stylesheet" />

    <!-- User Profile Photo -->
    <link href="~/Content/css/Main/netdna.bootstrapcdn.css" rel="stylesheet" />

    @*<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">*@
    <!-- Google font -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:200,400,700" rel="stylesheet">
    <link href="~/Content/SearchStyle.css" rel="stylesheet" />
    <link href="~/Content/ErrorrStyle.css" rel="stylesheet" />

</head>
<body>
    <amp-ad width="100vw" height="320"
            type="adsense"
            data-ad-client="ca-pub-2717829644533925"
            data-ad-slot="4018204669"
            data-auto-format="rspv"
            data-full-width="">
        <div overflow=""></div>
    </amp-ad>


    <!--================Header Menu Area =================-->
    <header class="header_area">
        <div class="top_menu row m0">
            <div class="container">
                <div class="float-left">
                    <a href="mailto:destek@reklamkafe.com">destek@reklamkafe.com</a>
                </div>
                <div class="float-right">
                    <ul class="header_social">
                        <li>
                            <a href="https://www.instagram.com/reklamkafee/" target="_blank"><i class="fa fa-instagram"></i> reklamkafee</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="main_menu">
            <nav class="navbar navbar-expand-lg navbar-light main_box">

                <div class="container">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <a class="navbar-brand logo_h" href="/anasayfa"><img src="~/Content/img/favicon.png" alt=""></a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse offset" id="navbarSupportedContent">
                        <ul id="ulMainNav" class="nav navbar-nav menu_nav ml-auto">
                            <li class="liMainNav nav-item active"><a class="nav-link" href="/anasayfa">Ana Sayfa</a></li>
                            <li class="liMainNav nav-item"><a class="nav-link" href="/urunler">Ürünler</a></li>
                            <li class="liMainNav nav-item"><a class="nav-link" href="/siralama">Puantaj</a></li>
                            <li class="liMainNav nav-item"><a class="nav-link" href="/iletisim">İLETİŞİM</a></li>

                            @if (Session["nickname"] == null || string.IsNullOrEmpty(Session["nickname"].ToString()) || string.IsNullOrWhiteSpace(Session["nickname"].ToString()))
                            {
                                <li class="nav-item"><a href="@Url.Action("Login","User")" class="nav-link">GİRİŞ YAP</a></li>
                            }
                            else
                            {
                                <li class="nav-item"><a class="nav-link" href="@Url.Action("Index","Home")">Reklam İzle</a></li>

                                <li class="nav-item submenu dropdown">
                                    <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">@(Session["nickname"] == null ? string.Empty : Session["nickname"])</a>
                                    <ul class="dropdown-menu">
                                        <li class="nav-item"><a class="nav-link" href="@Url.Action("UserInformation","User")">HESABIM</a>
                                        <li class="nav-item"><a class="nav-link" href="@Url.Action("LogOut","User")">Çıkış Yap</a>
                                    </ul>
                                </li>
                            }
                        </ul>

                        <div id="myOverlay" class="overlay">
                            <span class="closebtn" onclick="closeSearch()" title="Close Overlay">×</span>
                            <div class="overlay-content">
                                <form action="/bul/search" method="post">
                                    <input type="text" placeholder="Search .." name="search">
                                    <button type="submit"><i class="fa fa-search"></i></button>
                                </form>
                            </div>
                        </div>
                        <ul class="nav navbar-nav navbar-right">
                            <li><button class="openBtn lnr lnr-magnifier" onclick="openSearch()"></button></li>
                        </ul>

                    </div>
                </div>
            </nav>
            <div class="col-sm-12">
                @if (Win.AspMvcUI.Helpers.Session.SessionControl.IsActiveByUser())
                {
                    if (!(bool)Session["isAddressEmpty"])
                    {
                        <div class="alert alert-danger" style="text-align:center;">
                            <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                            <strong>Dikkat! Adres bilgilerini tamamlaman gerekiyor.</strong><a href="/bilgilerim" class="alert-link">Hesabıma git</a>
                        </div>
                    }
                    if ((bool)Session["AlertMessage"] == true)
                    {
                        Session["AlertMessage"] = false;
                        <div class="alert alert-warning" id="loginAlert" role="alert" style="text-align:center;font-weight:bold">Giriş Yapıldı</div>
                    }
                }
            </div>
        </div>
    </header>
    <!--================Header Menu Area =================-->

    @RenderBody()
    @Html.Partial("/Views/Partial/_StatusModal.cshtml") <!-- Bu partial'ın RenderBody den sonra gelmesi gerekiyor. -->
    <!--================ start footer Area  =================-->
    <footer class="footer-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-3  col-md-6 col-sm-6">
                    <div class="single-footer-widget">
                        <h6 class="footer_title">Hakkımızda</h6>
                        <p>Amacımız, elde ettiğimiz reklam gelirleri ile siz değerli kullanıcılarımıza ücretsiz olarak ürünler ulaştırabilmektir.</p>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-6">
                    <div class="single-footer-widget">
                        <h6 class="footer_title">Bülten</h6>
                        <p>Son gelişmeleri mail adresinize gönderelim.</p>
                        <div>
                            <form action="~/Home/BulletinAdd" method="post" class="subscribe_form relative">
                                <div class="input-group d-flex flex-row">
                                    <input name="Mail" placeholder="Email Adresiniz" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Email Adresiniz '" required="" type="email">
                                    <button type="submit" class="btn sub-btn"><span class="lnr lnr-arrow-right"></span></button>
                                </div>
                                <div class="mt-10 info"></div>
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 col-sm-6">
                    <div class="single-footer-widget f_social_wd">
                        <div class="single-footer-widget f_social_wd">
                            <h6 class="footer_title">Bilgi</h6>
                            <div class="f_social">
                                <p><a href="/gizlilik">Gizlilik Politikası</a></p>
                                <p><a href="/sartlarvekosullar">Şartlar ve Koşullar</a></p>
                                <p><a href="/sss">SSS</a></p>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="col-lg-2 col-md-6 col-sm-6">
                    <div class="single-footer-widget f_social_wd">
                        <h6 class="footer_title">Bizi Takip Edin</h6>
                        <p>Teslim edilen ürünler hesabımızda yayınlanacaktır.</p>
                        <div class="f_social">
                            <a href="https://www.instagram.com/reklamkafee/" target="_blank"><i class="fa fa-instagram"></i> reklamkafee</a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row footer-bottom d-flex justify-content-between align-items-center">
                <p class="col-lg-12 footer-text text-center">
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    ReklamKafe &copy;
                    @DateTime.Now.Year Tüm Hakları Saklıdır</a>
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                </p>
            </div>
        </div>
    </footer>
    <!--================ End footer Area  =================-->

    @RenderSection("JavaScripts", required: false)


</body>
</html>



<script type="text/javascript">
    $(document).ready(function () {
        window.setTimeout(function () {
            $("#loginAlert").fadeTo(1000, 0).slideUp(500, function () {
                $(this).remove();
            });
        }, 2500);
    });
</script>